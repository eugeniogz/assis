@prefix : <http://ws1.assis.bhz.br/gbd#>.
@prefix kb: <http://ws1.assis.bhz.br/gbd/kb#>.
@prefix obo: <http://purl.obolibrary.org/obo/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix sio: <http://semanticscience.org/resource/>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix hasco: <http://hadatac.org/ont/hasco/>.

<http://ws1.assis.bhz.br/gbd#> a owl:Ontology;
  owl:versionIRI <http://ws1.assis.bhz.br/gbd/0.1>.

hasco:StudyIndicator rdf:type owl:Class.
:EnviromentVariables rdfs:subClassOf  hasco:StudyIndicator.
:SocioEconomicVariables rdfs:subClassOf  hasco:StudyIndicator.
:DiseaseIndicators rdfs:subClassOf  hasco:StudyIndicator.

:Biome rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:Region rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:State rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:hasRegion  rdf:type owl:ObjectProperty; rdfs:domain :State; rdfs:range :Region.
:hasBiome  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :Biome.
:hasState  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :State.
:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :SocioEconomicVariables; rdfs:range hasco:NamedTime.
hasco:NamedTime rdf:type owl:Class.

:County rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:CountyName rdfs:subClassOf :SocioEconomicVariables.
:hasName rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :CountyName.
:residencesWithInadequateSanitation  rdfs:subClassOf :SocioEconomicVariables.
:hasResidencesWithInadequateSanitation rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :residencesWithInadequateSanitation.
:totalBasicAtentionUnits rdfs:subClassOf :SocioEconomicVariables.
:hasTotalBasicAtentionUnits rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :totalBasicAtentionUnits.
:numberOfDeathsLessThanOneYear rdfs:subClassOf :SocioEconomicVariables.
:hasNumberOfDeathsLessThanOneYear rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :numberOfDeathsLessThanOneYear.
:hasTotalPopulation  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :TotalPopulation.
:TotalPopulation rdf:type owl:Class.
:hasDiseaseIndicators  rdf:type owl:ObjectProperty; rdfs:domain :TotalPopulation; rdfs:range :DiseaseIndicators.
:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :DiseaseIndicators; rdfs:range hasco:NamedTime.

:Schistosomiasis  rdf:type owl:Class; rdfs:subClassOf :DiseaseIndicators.
:totalSchistosomiasisCases rdfs:subClassOf :SocioEconomicVariables.
:hasTotalSchistosomiasisCases rdf:type owl:ObjectProperty; rdfs:domain :Schistosomiasis; rdfs:range :totalSchistosomiasisCases.
:totalSchistosomiasisOn100Thousand rdfs:subClassOf :SocioEconomicVariables.
:hasTotalSchistosomiasisOn100Thousand rdf:type owl:ObjectProperty; rdfs:domain :Schistosomiasis; rdfs:range :totalSchistosomiasisOn100Thousand.

:Dengue  rdf:type owl:Class; rdfs:subClassOf :DiseaseIndicators.
:totalDengueCases rdfs:subClassOf :SocioEconomicVariables.
:hasTotalDengueCases rdf:type owl:ObjectProperty; rdfs:domain :Dengue; rdfs:range :totalDengueCases.

:totalDengueCasesOn100Thousand rdfs:subClassOf :SocioEconomicVariables.
:hasTotalDengueCasesOn100Thousand rdf:type owl:ObjectProperty;  rdfs:domain :Dengue; rdfs:range :totalDengueCasesOn100Thousand.

:Dengue1 rdf:type :Dengue;
:totalDengueCases "42".

:Surface rdf:type owl:Class; rdfs:subClassOf :EnviromentVariables.
:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :EnviromentVariables; rdfs:range hasco:NamedTime.

:hasSurface rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :Surface.

:averageTemperature   rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range rdfs:Literal.
:averagePrecipitation   rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range rdfs:Literal.
:normalizedDifferenceVegetationIndex   rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range rdfs:Literal.
:normalizedDifferenceWaterIndex   rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range rdfs:Literal.


:population25PctMinimalIncome rdfs:subClassOf :SocioEconomicVariables.
:hasPopulation25PctMinimalIncome rdf:type owl:ObjectProperty;  rdfs:domain :TotalPopulation; rdfs:range :population25PctMinimalIncome.
:Iliterates rdfs:subClassOf :SocioEconomicVariables.
:hasIliterates rdf:type owl:ObjectProperty;  rdfs:domain :Greater15Population; rdfs:range :Iliterates.
:Greater15Population a owl:Class; rdfs:subClassOf :TotalPopulation.

#SDD Relations
:isTotalPopulationOf rdfs:domain :County;
 rdfs:range :TotalPopulation.
