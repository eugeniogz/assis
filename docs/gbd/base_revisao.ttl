:Biome rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:Region rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:State rdf:type owl:Class; rdfs:subClassOf :SocioEconomicVariables.
:hasRegion  rdf:type owl:ObjectProperty; rdfs:domain :State; rdfs:range :Region.
:hasBiome  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :Biome.
:hasState  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :State.
:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :SocioEconomicVariables; rdfs:range hasco:NamedTime.
hasco:NamedTime rdf:type owl:Class.

:County rdf:type owl:Class.
:CountyName rdfs:subClassOf :SocioEconomicVariables.
:hasName rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :CountyName.
:ResidencesWithInadequateSanitation  rdfs:subClassOf :SocioEconomicVariables.
:hasResidencesWithInadequateSanitation rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :ResidencesWithInadequateSanitation.
:TotalBasicAtentionUnits rdfs:subClassOf :SocioEconomicVariables.
:hasTotalBasicAtentionUnits rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :TotalBasicAtentionUnits.
:NumberOfDeathsLessThanOneYear rdfs:subClassOf :SocioEconomicVariables.
:hasNumberOfDeathsLessThanOneYear rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :NumberOfDeathsLessThanOneYear.
:hasTotalPopulation  rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :TotalPopulation.
:TotalPopulation rdf:type owl:Class.

:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :DiseaseIndicators; rdfs:range hasco:NamedTime.

:Schistosomiasis  rdf:type owl:Class.
:TotalSchistosomiasisCases rdfs:subClassOf :DiseaseIndicators.
:isRelatedTo rdf:type owl:ObjectProperty; rdfs:domain :TotalSchistosomiasisCases; rdfs:range :Schistosomiasis.
:TotalSchistosomiasisOn100Thousand rdfs:subClassOf :DiseaseIndicators.
:isRelatedTo rdf:type owl:ObjectProperty; rdfs:domain :TotalSchistosomiasisOn100Thousand; rdfs:range :Schistosomiasis.

:Dengue  rdf:type owl:Class.
:TotalDengueCases rdfs:subClassOf :DiseaseIndicators.
:isRelatedTo rdf:type owl:ObjectProperty; rdfs:domain :TotalDengueCases; rdfs:range :Dengue.
:TotalDengueCasesOn100Thousand rdfs:subClassOf :DiseaseIndicators.
:isRelatedTo rdf:type owl:ObjectProperty;  rdfs:domain :TotalDengueCasesOn100Thousand; rdfs:range :Dengue.

:hasTotalSchistosomiasisCases rdf:type owl:ObjectProperty; rdfs:domain :TotalPopulation; rdfs:range :TotalSchistosomiasisCases.
:hasTotalSchistosomiasisOn100Thousand rdf:type owl:ObjectProperty; rdfs:domain :TotalPopulation; rdfs:range :TotalSchistosomiasisOn100Thousand.
:hasTotalDengueCases rdf:type owl:ObjectProperty; rdfs:domain :TotalPopulation; rdfs:range :TotalDengueCases.
:hasTotalDengueCasesOn100Thousand rdf:type owl:ObjectProperty;  rdfs:domain :TotalPopulation; rdfs:range :TotalDengueCasesOn100Thousand.


:Dengue1 rdf:type :Dengue;
:totalDengueCases "42".

:Surface rdf:type owl:Class.
:hasYear  rdf:type owl:ObjectProperty; rdfs:domain :EnviromentVariables; rdfs:range hasco:NamedTime.
:hasSurface rdf:type owl:ObjectProperty; rdfs:domain :County; rdfs:range :Surface.

:AverageTemperature rdfs:subClassOf :EnviromentVariables.
:hasAverageTemperatura  rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range :AverageTemperature.
:AveragePrecipitation rdfs:subClassOf :EnviromentVariables.
:hasAveragePrecipitation  rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range :AveragePrecipitation.
:NormalizedDifferenceVegetationIndex rdfs:subClassOf :EnviromentVariables.
:hasNormalizedDifferenceVegetationIndex  rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range :NormalizedDifferenceVegetationIndex.
:NormalizedDifferenceWaterIndex rdfs:subClassOf :EnviromentVariables.
:hasNormalizedDifferenceWaterIndex  rdf:type owl:ObjectProperty;  rdfs:domain :Surface; rdfs:range :NormalizedDifferenceWaterIndex.


:Population25PctMinimalIncome rdfs:subClassOf :SocioEconomicVariables.
:hasPopulation25PctMinimalIncome rdf:type owl:ObjectProperty;  rdfs:domain :TotalPopulation; rdfs:range :Population25PctMinimalIncome.
:Iliterates rdfs:subClassOf :SocioEconomicVariables.
:hasIliterates rdf:type owl:ObjectProperty;  rdfs:domain :Greater15Population; rdfs:range :Iliterates.
:Greater15Population rdfs:subClassOf :TotalPopulation.