@prefix : <http://assis.bhz.br/ont/ontobras#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix hasco: <http://hadatac.org/ont/hasco/> .

<http://assis.bhz.br/ont/ontobras> a owl:Ontology ;
	owl:versionIRI <http://assis.bhz.br/ont/ontobras/0.1> ;
	owl:imports <http://semanticscience.org/ontology/sio-subset-labels.owl> , <http://hadatac.org/ont/hasco/> , <http://semanticscience.org/ontology/sio.owl> .

#################################################################
#
#    Annotation properties
#
#################################################################

skos:definition a owl:AnnotationProperty .

:Age a owl:Class;
   	rdfs:label "Age";
	rdfs:subClassOf  hasco:StudyIndicator;
	skos:definition "Age".

:SolutionPH a owl:Class;
	rdfs:label "Solution PH";
  	rdfs:subClassOf hasco:StudyIndicator ;
	skos:definition "Solution PH".

:EducationLevel a owl:Class;
	rdfs:label "Education Level";
	rdfs:subClassOf  hasco:StudyIndicator;
	skos:definition "Education Level".

:Urine a owl:Class;
        rdfs:label "Urine";
        skos:definition "Urine".


:Month a owl:Class;
        rdfs:label "Month";
	skos:definition "Month".

:hasMother a rdf:Property;
        rdfs:label "has Mother".

:hasChild a rdf:Property;
        rdfs:label "has Child". 

:Month1 a demo:Month;
        rdfs:label "Month 1";
        skos:definition "Month 1".
	 
:Month2 a demo:Month;
        rdfs:label "Month 2";
        skos:definition "Month 2".

######################################################
# Deprecated Classes used by versions earlier than V7
######################################################

:Sample1 a owl:Class;
        rdfs:label "Sample 1";
        skos:definition "Sample 1".
	 
:Sample2 a owl:Class;
        rdfs:label "Sample 2";
        skos:definition "Sample 2".

################################################
#
#   Quotes 
#
################################################

sio:Human a owl:Class;
    rdfs:label "Human";
    skos:definition "A Human being".
